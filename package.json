{
  "name": "star8080-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "play:dev": "pnpm run --filter playground dev",
    "star:coverage": "pnpm run --filter star8080-ui coverage",
    "star:test": "pnpm run --filter star8080-ui test",
    "star:build": "gulp -f packages/components/script/build/gulpfile.ts",
    "docs:dev": "pnpm run --filter docs dev",
    "docs:build": "pnpm run --filter docs build",
    "docs:serve": "pnpm run --filter docs serve",
    "lint:ts": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:style": "stylelint \"./**/*.{css,less,vue,html}\" --fix",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md,less}\""
  },
  "lint-staged": {
    "./**/*.{js,ts,jsx,tsx,vue}": [
      "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix"
    ],
    "./**/*.{css,less,vue,html}": [
      "stylelint \"./**/*.{css,less,vue,html}\" --fix"
    ],
    "./**/*.{html,vue,ts,js,json,md,less}": [
      "prettier --write \"./**/*.{html,vue,ts,js,json,md,less}\""
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/less": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vue/test-utils": "^2.2.10",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^8.2.6",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.1",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.4",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^6.0.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.28.4",
    "vue": "^3.2.36"
  }
}
